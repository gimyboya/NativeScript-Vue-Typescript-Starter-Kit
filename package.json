{
  "name": "mobile-starter",
  "version": "0.0.0-development",
  "description": "A NativeScript-Vue + Typescript starter kit for getting up and running quickly",
  "author": "gimyboya <b.afatah@yahoo.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/gimyboya/NativeScript-Vue-Typescript-Starter-Kit.git"
  },
  "nativescript": {
    "id": "org.starter",
    "tns-ios": {
      "version": "5.0.0"
    },
    "tns-android": {
      "version": "5.1.0"
    }
  },
  "scripts": {
    "clean-run:android": "tns run android --bundle --clean",
    "clean-run:ios": "tns run ios --bundle --clean",
    "commit": "npx git-cz",
    "release": "semantic-release",
    "run:android": "tns run android --bundle",
    "run:ios": "tns run ios --bundle",
    "watch:android": "tns run android --bundle --hmr",
    "watch:ios": "tns run ios --bundle --hmr",
    "build:android": "tns build android --bundle --production",
    "build:ios": "tns build ios --bundle --production",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@vue/devtools": "5.0.0-beta.3",
    "nativescript-socket.io": "^0.9.0",
    "nativescript-theme-core": "^1.0.4",
    "nativescript-toast": "^1.4.6",
    "nativescript-ui-sidedrawer": "^4.3.0",
    "nativescript-vue": "^2.0.0",
    "nativescript-vue-devtools": "^1.0.0",
    "tns-core-modules": "^5.0.2",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.2.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/node": "^10.12.18",
    "babel-loader": "^8.0.2",
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "babylon": "6.18.0",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^3.0.5",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "husky": "^1.3.1",
    "lazy": "1.0.11",
    "nativescript-dev-typescript": "^0.7.9",
    "nativescript-dev-webpack": "next",
    "nativescript-vue-template-compiler": "^2.0.0",
    "nativescript-worker-loader": "~0.9.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.13.2",
    "terser-webpack-plugin": "^1.1.0",
    "tns-platform-declarations": "^5.1.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "vue": "^2.5.21",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.16.4",
    "webpack-bundle-analyzer": "~2.13.1",
    "webpack-cli": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
